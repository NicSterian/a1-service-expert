{
    "name":  "booking-api",
    "private":  true,
    "version":  "0.0.1",
    "description":  "NestJS API for the A1 Service Expert booking platform",
    "scripts":  {
                    "dev":  "nest start --watch --entryFile main",
                    "build":  "nest build",
                    "start":  "node dist/main.js",
                    "prisma:g":  "prisma generate",
                    "prisma:migrate":  "prisma migrate dev",
                    "prisma:seed":  "prisma db seed",
                    "test":  "jest"
                },
    "prisma":  {
                   "seed":  "ts-node --transpile-only prisma/seed.ts"
               },
    "dependencies":  {
                         "@a1/shared":  "workspace:*",
                         "@nestjs/common":  "^10.4.5",
                         "@nestjs/config":  "^3.2.0",
                         "@nestjs/core":  "^10.4.5",
                         "@nestjs/jwt":  "^10.2.0",
                         "@nestjs/platform-express":  "^10.4.5",
                         "@prisma/client":  "^5.16.1",
                         "bcryptjs":  "^2.4.3",
                         "class-transformer":  "^0.5.1",
                         "class-validator":  "^0.14.1",
                         "dotenv":  "^16.4.5",
                         "ioredis":  "^5.4.1",
                         "reflect-metadata":  "^0.2.2",
                         "nodemailer":  "^6.9.13",
                         "rxjs":  "^7.8.1",
                         "pdfkit":  "^0.13.0"
                     },
    "devDependencies":  {
                            "@nestjs/cli":  "^10.3.2",
                            "@nestjs/schematics":  "^10.1.1",
                            "@nestjs/testing":  "^10.4.5",
                            "@types/bcryptjs":  "^2.4.2",
                            "@types/express":  "^5.0.3",
                            "@types/jest":  "^29.5.12",
                            "@types/node":  "^20.14.9",
                            "@types/nodemailer":  "^6.4.15",
                            "jest":  "^29.7.0",
                            "prisma":  "^5.16.1",
                            "ts-jest":  "^29.1.1",
                            "ts-node":  "^10.9.2",
                            "typescript":  "^5.4.5",
                            "@types/pdfkit":  "^0.12.11"
                        }
}
